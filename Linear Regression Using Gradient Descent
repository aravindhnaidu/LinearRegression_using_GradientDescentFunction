nerate y values with given slope and intercept\n","def generate_y(slope, intercept):\n","    return slope * x + intercept + noise\n","\n","# Iterating over different slopes and intercepts\n","slopes = [2.5, 3.0, 3.5, 4.0, 4.5]\n","intercepts = [1.0, 1.5, 2.0, 2.5, 3.0]\n","\n","plt.figure(figsize=(12, 8))\n","\n","for slope, intercept in zip(slopes, intercepts):\n","    y = generate_y(slope, intercept)\n","    plt.scatter(x, y, label=f'Slope: {slope}, Intercept: {intercept}')\n","\n","plt.title('Linear Regression with Different Slopes and Intercepts')\n","plt.xlabel('X')\n","plt.ylabel('Y')\n","plt.legend()\n","plt.grid(True)\n","plt.show()\n"],"metadata":{"id":"B7686lavzLlh","colab":{"base_uri":"https://localhost:8080/","height":718},"executionInfo":{"status":"ok","timestamp":1721104881011,"user_tz":-330,"elapsed":2114,"user":{"displayName":"Aravindh Pandu","userId":"13725647323903316920"}},"outputId":"4df692ab-3e1f-4b07-d3c9-0ea87e6d34aa"},"execution_count":1,"outputs":[{"output_type":"display_data","data":{"text/plain":["<Figure size 1200x800 with 1 Axes>"],"image/png":"\n"},"metadata":{}}]},{"cell_type":"code","source":["import numpy as np\n","import matplotlib.pyplot as plt\n","\n","# Set random seed for reproducibility\n","np.random.seed(0)\n","\n","# Number of data points\n","n = 100\n","\n","# Generate x values between 0 and 10\n","x = np.random.uniform(0, 10, n)\n","\n","# Generate random noise from a normal distribution\n","noise = np.random.normal(0, 1, n)\n","\n","# Function to generate y values with given slope and intercept\n","def generate_y(slope, intercept):\n","    return slope * x + intercept + noise\n","\n","# Iterating over different slopes and intercepts\n","slopes = [4.5,4.0,3.5,3.0,2.5,2.0,1.5,1.0]\n","intercepts = [3.0,2.5,2.0,1.5,1.0]\n","\n","plt.figure(figsize=(12, 8))\n","\n","for slope, intercept in zip(slopes, intercepts):\n","    y = generate_y(slope, intercept)\n","    plt.scatter(x, y, label=f'Slope: {slope}, Intercept: {intercept}'